{
  "expo": {
    "name": "Field Data Collector",
    "slug": "field-data-collector",
    "version": "2.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00122a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vittorjavidan.fielddatacollector"
    },
    "android": {
      "package": "com.vittorjavidan.fielddatacollector",
      "versionCode": 23,
      "backgroundColor": "#000000",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00122a"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCEgCnCZM5a2iyk2O8sr4iMaVT4Ulq9LX4"
        }
      }
    },
    "scheme": "myapp",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c2eb23bc-0277-4046-b205-388e321e6839"
      }
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-localization",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "owner": "vittorjavidan"
  }
}
