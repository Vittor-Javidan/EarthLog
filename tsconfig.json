{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@Types/*": [
        "Types/*"
      ],
      "@Components/*": [
        "Components/*"
      ],
      "@Services/*": [
        "Services/*"
      ],
      "@Globals/*": [
        "Globals/*"
      ],
      "@Translations/*": [
        "Translations/*"
      ],
      "@Icon/*": [
        "Components/Icon/*"
      ],
      "@Layout/*": [
        "Components/Layout/*"
      ],
      "@Inputs/*": [
        "Components/Inputs/*"
      ],
      "@Widget/*": [
        "Components/Widget/*"
      ],
      "@WidgetComponents/*": [
        "Components/Widget/Components/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
