{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@Components/*": ["Components/*"],
      "@Services/*": ["Services/*"],
      "@Globals/*": ["Globals/*"],
      "@Icon/*": ["Components/Icon/*"],
      "@Layout/*": ["Components/Layout/*"],
      "@Inputs/*": ["Components/Inputs/*"],
      "@Widget/*": ["Components/Widget/*"],
      "@WidgetComponents/*": ["Components/Widget/Components/*"],
    },
  }
}
