{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@Components/*": ["Components/*"],
      "@Services/*": ["Services/*"],
      "@AppRoutes/*": ["AppRoutes/*"],
